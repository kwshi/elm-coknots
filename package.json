{
  "name": "elm-coknots",
  "version": "1.0.0",
  "description": "Draw some cool knots",
  "devDependencies": {
    "cpx": "^1.5.0",
    "elm": "^0.19.1-5",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "dev:cp": "cpx --watch 'src/static/**/*.{html,js}' 'dist'",
    "dev:elm": "elm-live -Hd 'dist' -- --debug --output=dist/elm.js src/Main.elm",
    "dev": "run-p dev:cp dev:elm",
    "build:cp": "cpx 'src/static/**/*.{html,js}' 'dist'",
    "build:elm": "elm make --output=dist/elm.js src/Main.elm",
    "build": "run-p build:cp build:elm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwshi/elm-coknots.git"
  },
  "keywords": ["elm", "knot", "gauss", "colinear"],
  "author": "al. et. Kye Shi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwshi/elm-coknots/issues"
  },
  "homepage": "https://github.com/kwshi/elm-coknots#readme"
}
